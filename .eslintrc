rules:
  strict: 0
  new-cap: 0
  quotes: 0
  semi: 0
  global-strict: 0
  comma-dangle: 0
  camelcase: 0
  no-unused-vars: 0
  curly: 0
  no-console: 0
  no-trailing-spaces: 0
  no-underscore-dangle: 0
  no-unneeded-ternary: 1
  space-infix-ops: 0
  no-use-before-define: 0
  no-extend-native: 0
  comma-spacing: 0
  no-new: 0
  no-mixed-spaces-and-tabs: 0

  no-multi-spaces: 1
  no-constant-condition: 0
globals:
  require: true
  module: true
  console: true
  setTimeout: true
  setInterval: true
  __dirname: true
  Promise: true
